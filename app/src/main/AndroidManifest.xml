<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.xuender.ethics.app">

    <application
        android:name=".EthicsApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".five.FiveActivity"
            android:label="@string/five"
            android:icon="@drawable/ic_five" />
        <activity
            android:name=".notes.ZhiActivity"
            android:label="@string/zhi"
            android:icon="@drawable/ic_zhi" />
        <activity
            android:name=".notes.YiActivity"
            android:label="@string/yi"
            android:icon="@drawable/ic_yi" />
        <activity
            android:name=".settings.SettingsActivity"
            android:label="@string/setting"
            android:icon="@drawable/ic_menu_manage" />
    </application>
</manifest>